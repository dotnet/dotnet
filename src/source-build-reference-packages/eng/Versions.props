<Project>
  <PropertyGroup>
    <MajorVersion>10</MajorVersion>
    <MinorVersion>0</MinorVersion>
    <PatchVersion>0</PatchVersion>
    <SkipPackagePublishingVersionChecks>true</SkipPackagePublishingVersionChecks>
    <!-- Opt-out repo features -->
    <UsingToolXliff>false</UsingToolXliff>
  </PropertyGroup>
  <!-- External package versions -->
  <PropertyGroup>
    <AbstractionsXunitReleaseVersion>2.0.3</AbstractionsXunitReleaseVersion>
    <AzureActiveDirectoryIdentityModelExtensionsForDotNetReleaseVersion>8.0.1</AzureActiveDirectoryIdentityModelExtensionsForDotNetReleaseVersion>
    <CssParserReleaseVersionSuffix>20230414.1</CssParserReleaseVersionSuffix>
    <HumanizerReleaseVersion>2.14.1</HumanizerReleaseVersion>
    <MSBuildLocatorReleaseVersion>1.6.10</MSBuildLocatorReleaseVersion>
    <SolutionPersistenceVersion>1.0.52</SolutionPersistenceVersion>
    <SpectreConsoleReleaseVersion>0.48.0</SpectreConsoleReleaseVersion>
    <XunitReleaseVersion>2.9.2</XunitReleaseVersion>
  </PropertyGroup>
  <PropertyGroup>
    <!-- External package dependencies -->
    <HumanizerCoreVersion>$(HumanizerReleaseVersion)</HumanizerCoreVersion>
    <!-- MSBuild dependencies -->
    <MicrosoftBuildVersion>17.15.0-preview-25351-106</MicrosoftBuildVersion>
    <MicrosoftBuildUtilitiesCoreVersion>17.15.0-preview-25351-106</MicrosoftBuildUtilitiesCoreVersion>
    <!-- NuGet dependencies -->
    <NuGetPackagingVersion>6.11.0</NuGetPackagingVersion>
    <!-- Runtime dependencies -->
    <MicrosoftNETCoreILAsmVersion>10.0.0-preview.7.25351.106</MicrosoftNETCoreILAsmVersion>
    <MicrosoftNETCoreILDAsmVersion>10.0.0-preview.7.25351.106</MicrosoftNETCoreILDAsmVersion>
    <!-- SDK dependencies -->
    <MicrosoftDotNetGenAPITaskPackageVersion>10.0.100-preview.7.25351.106</MicrosoftDotNetGenAPITaskPackageVersion>
    <!-- xUnit dependencies -->
    <XunitSkippableFactVersion>1.4.13</XunitSkippableFactVersion>
  </PropertyGroup>
</Project>
